# Google Gemini API Key
GOOGLE_API_KEY=

# Firestore Credentials (optional, if using cloud storage)
# Leave blank to use in-memory storage fallback

# Vertex AI (Gemini on Vertex) - enable to use GCP quotas instead of public Gemini
# Set GOOGLE_GENAI_USE_VERTEXAI=true and provide project/location + credentials.
AGENT_ENABLE_LLM=
# Defaults to "test_user" if unset; should match the profile user_id in Firestore.
AGENT_USER_ID=
# If set, Vertex is used as a fallback when public Gemini fails.
AGENT_USE_VERTEX=
GOOGLE_GENAI_USE_VERTEXAI=
VERTEX_PROJECT_ID=
VERTEX_LOCATION=
GEMINI_MODEL_NAME=
# Path inside the backend container; matched by docker-compose volume
GOOGLE_APPLICATION_CREDENTIALS=
# Use Firestore; database ID must match the one you created
GCLOUD_PROJECT=
FIRESTORE_DATABASE_ID=
